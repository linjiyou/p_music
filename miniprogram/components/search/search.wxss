.container{
  display: flex;
  align-items: center;
  margin-left: 20rpx;
  position: relative;
}
.bar{
  flex: 1; 
  height: 68rpx;
  font-size: 28rpx;
  border-radius: 50px;
  background-color: #f5f5f5;
  padding-left: 60rpx;
  
  
}
.search{
  background-color: #d43c33;
  color: #fff;
  height: 60rpx;
  padding: 0 30rpx ;
  border-radius: 10rpx;
  text-align: center;
  font-size: 24rpx;
  line-height: 60rpx;
  margin-left: 16rpx;
}
.in-bar{
  color: #999;
}
.find{
  position: absolute;
  top: 50%;
  transform: translate(16rpx,-50%);
  color: #9f9f9f;
}